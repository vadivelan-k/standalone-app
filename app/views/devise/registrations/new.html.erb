<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
<table class="tcontent" width="100%" border="0" cellspacing="0" cellpadding="0">
  <tr>
    <td align="center" valign="top"><h2>Sign up</h2><center><%= devise_error_messages! %></center><table width="60%" border="0" cellspacing="0" cellpadding="8">
      <tr>
        <td width="16%" align="left" valign="top"><%= f.label :email %> </td>
        <td width="20%" align="left" valign="top"><%= f.email_field :email %></td>
      </tr>
			
			<tr>
			  <td align="left" valign="top"><%= f.label :password %></td>
				<td align="left" valign="top"> <%= f.password_field :password %></td>          
			</tr>
			
			<tr>
			  <td align="left" valign="top"><%= f.label :password_confirmation %></td>
				<td align="left" valign="top"> <%= f.password_field :password_confirmation %></td>          
			</tr>
			
			<tr>
			  <td align="left" valign="top"><%= f.label :first_name %></td>
				<td align="left" valign="top"> <%= f.password_field :first_name %></td>          
			</tr>
			
			<tr>
			  <td align="left" valign="top"><%= f.label :last_name %></td>
				<td align="left" valign="top"> <%= f.password_field :last_name %></td>          
			</tr>		
			
			<tr>
			  <td align="left" valign="top"></td>
				<td align="left" valign="top"> <%= f.submit "Sign up" %></td>          
			</tr>  			
      </td></tr>		
    </table>
	</td>	
	</tr>
	</table>
 <%end%> 

<%= render :partial => "devise/shared/links" %>

